{
    "name": "phpinnacle/ensign",
    "type": "library",
    "description": "PHPinnacle Signal Dispatcher",
    "keywords": [
        "phpinnacle", "async", "signal dispatcher"
    ],
    "homepage": "https://github.com/phpinnacle/ensign",
    "license": "MIT",
    "authors": [
        {
            "name": "PHPinnacle",
            "email": "dev@phpinnacle.com",
            "homepage": "https://phpinnacle.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "amphp/amp": "^2.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "amphp/parallel-functions": "^0.1.3",
        "phpunit/phpunit": "^6.0"
    },
    "suggest": {
        "amphp/parallel-functions": "For using ParallelProcessor."
    },
    "autoload": {
        "psr-4": {
            "PHPinnacle\\Ensign\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPinnacle\\Ensign\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
